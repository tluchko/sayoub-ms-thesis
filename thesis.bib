@article{wangMagneticIronOxide2014,
  title = {Magnetic Iron Oxide–Fluorescent Carbon Dots Integrated Nanoparticles for Dual-Modal Imaging, near-Infrared Light-Responsive Drug Carrier and Photothermal Therapy},
  author = {Wang, Hui and Shen, Jing and Li, Yingyu and Wei, Zengyan and Cao, Guixin and Gai, Zheng and Hong, Kunlun and Banerjee, Probal and Zhou, Shuiqin},
