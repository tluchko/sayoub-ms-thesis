@article{swailsConstantPHReplica2014,
  title = {Constant {{pH Replica Exchange Molecular Dynamics}} in {{Explicit Solvent Using Discrete Protonation States}}: {{Implementation}}, {{Testing}}, and {{Validation}}},
  shorttitle = {Constant {{pH Replica Exchange Molecular Dynamics}} in {{Explicit Solvent Using Discrete Protonation States}}},
  author = {Swails, Jason M. and York, Darrin M. and Roitberg, Adrian E.},
  date = {2014-03-11},
  journaltitle = {Journal of Chemical Theory and Computation},
