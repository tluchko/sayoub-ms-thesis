  author = {Gelis, Ioannis and Bonvin, Alexandre M. J. J. and Keramisanou, Dimitra and Koukaki, Marina and Gouridis, Giorgos and Karamanou, Spyridoula and Economou, Anastassios and Kalodimos, Charalampos G.},
  date = {2007-11-16},
