\begin_inset Formula 
\begin{equation}
\Delta G^{\circ}\approx\Delta A^{\circ}=-k_{B}Tln\frac{\int_{\Gamma_{1}}e^{-\beta U(x)}dx}{\int_{\Gamma_{0}}e^{-\beta U(x)}dx}.\label{eq:final-two-state-binding}
\end{equation}

\end_inset


