\begin_inset Formula 
\begin{align*}
\Delta F_{10} & =-\frac{1}{\beta}\text{ln\left[\frac{\int e^{-\beta\left(U_{1}(q)-U_{0}(q)+U_{0}(q)\right)dq}}{Q_{0}}\right]}\\
 & =-\frac{1}{\beta}\text{ln\left[\frac{\int e^{-\beta\left(U_{1}(q)-U_{0}(q)\right)}e^{-\beta\left(U_{0}(q)\right)}dq}{Q_{0}}\right]}
\end{align*}

